include libs/linux_x86/io.corth
include libs/core.corth

proc fib
  int -- int
in let i in
  // Finds the nth fibonacci number using recursion

  i 2 <= if 1 else i dec fib i dec dec fib + end
end end

proc main
  int int -- int
in let argc argv in
  // Find the 10th fibonacci number
  10 fib putu "\n" puts
end 0 end
