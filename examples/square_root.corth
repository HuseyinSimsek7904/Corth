include libs/linux_x86/io.corth
include libs/math/fixed64.corth

proc main
  int int -- int
in let argc argv in
  "Enter an unsigned integer: " puts
  getu if int-to-fixed64 let x in
    "Square root of " puts x put-ufixed64 " is " puts x sqrt-f put-ufixed64 ".\n" puts
    "Calculated that with " puts SQRT.TOLERANCE put-ufixed64 " precision.\n" puts
  end else
    drop
  end
end 0 end
