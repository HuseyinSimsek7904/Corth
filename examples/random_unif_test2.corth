include libs/linux_x86/io.corth
include libs/random.corth


macro CHARS     " .,-+*%@" drop          endmacro
macro CHAR-SIZE 0x2000000000000000 endmacro


proc main
  int int -- int
in let argc argv in
  64 while dup isn-zero do
    64 while dup isn-zero do
      rand64 CHAR-SIZE / CHARS + 1 puts
    dec end drop
    "\n" puts
  dec end drop
end 0 end
