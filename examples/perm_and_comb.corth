include libs/linux_x86/io.corth
include libs/core/stack.corth
include libs/math/arithmetic.corth

proc main
  int int -- int
in let argc argv in
  "Enter A: " puts
  while getu ! do drop end
  "Enter B: " puts
  while getu ! do drop end

  let a b in
    "P(" puts a putu ", " puts b putu ") = " puts a b perm putu "\n" puts
    "C(" puts a putu ", " puts b putu ") = " puts a b comb putu "\n" puts
  end
end 0 end
