/*

    ======================
    ==== Name example ====
    ======================

Hello, what is your name?
Hüseyin
Hello, Hüseyin!

*/

include libs/sys.corth
include libs/linux/io.corth


memory name 32 end

proc main
    -- int
in
    "Hello, what is your name?\n" puts
    name getl

    "Hello, " puts
    name swp puts
    "!\n" puts
    
    0
end
