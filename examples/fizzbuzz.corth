include "linux_x86/stdio.corth"

proc main
  int int -> int
in let argc argv in
  memory char 1 in
    1 while char 1 gets drop char @8 '\n' = do let i in
      i 15 % is-zero if
        "FizzBuzz\n" puts
      else i 3 % is-zero if
        "Fizz\n" puts
      else i 5 % is-zero if
        "Buzz\n" puts
      else
        i putu putnl
      end end end
    i end inc end drop
  end
end 0 end
